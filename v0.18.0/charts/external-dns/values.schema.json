      "type": [
        "string"
      ],
      "enum": [
        "sync",
        "upsert-only"
      ]
    },
    "priorityClassName": {
      "description": "Priority class name for the `Pod`.",
      "type": [
        "string",
        "null"
      ]
    },
    "provider": {
