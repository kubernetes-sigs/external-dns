      "description": "Record types to manage (default: A, AAAA, CNAME)",
      "type": [
        "array",
        "null"
      ],
      "uniqueItems": true,
      "items": {
        "type": "string"
      }
    },
    "nameOverride": {
      "description": "Override the name of the chart.",
      "type": [
        "string",
        "null"
      ]
    },
